print ("please enter a number (end with -1): ");
String str = System.console().readLine();
int a = Integer.parseInt(str);
String sequence = ("");
while (a > -1) {
	print ("and another: ");
	String strs = System.console().readLine();
	int b = Integer.parseInt(strs);
	if (b == -1) {
		a = b-1;
	}
	if (a != b-1) {
		sequence = ("No");
	}
	a = b;
}
if (sequence.length() < 2) {
	sequence = ("Yes");
}
println (sequence);