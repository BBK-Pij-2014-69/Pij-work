print ("please enter a number (end with -1): ");
String str = System.console().readLine();
int a = Integer.parseInt(str);
int highNum = a;
while (a > -1) {
	print ("and another: ");
	str = System.console().readLine();
	a = Integer.parseInt(str);
	if (a > highNum){
		highNum = a;
	}
}
println ("The highest number entered was: " + highNum);